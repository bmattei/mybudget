 <%= turbo_frame_tag dom_id(category), class: "table-row even:bg-blue-100" do %>
  <div class="table-cell overflow-y-scroll px-2 py-1" >
    <%= category.name %>
  </div>
  <div class="table-cell overflow-y-scroll px-2 py-1" >
    <%= category.active %>
  </div>
  <div class="table-cell overflow-y-scroll px-2 py-1" >
    <%= category.super %>
  </div>
  <div class="table-cell">
    <%= link_to 'Show', url_for(category), data: {turbo_frame: "_top"}, class: "btn-small"%>
  </div>
  <div class="table-cell">
    <%= link_to 'Edit', url_for([:edit, category]), class: "btn-small"%>
  </div>
  <div class="table-cell">
   <%= link_to 'Delete', url_for(category), method: :delete,  data: {turbo_frame: "_top",  turbo_method: :delete, turbo_confirm: "Are you sure?"}, class: "btn-small" %>
  </div>
  <%= turbo_stream.update "flash" do %>
  <% flash.each do |flash_type, message| %>
      <div class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block"><%= message %></div>
  </div>
  <% end %>
  <% end %>
<% end %>
